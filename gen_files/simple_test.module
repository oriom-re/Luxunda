
def init(being_context=None):
    """Initialize test being"""
    if being_context:
        return {
            "status": "initialized",
            "being_alias": being_context.get('alias', 'test'),
            "message": "Test being initialized successfully"
        }
    return {"status": "initialized"}

def execute(data=None, **kwargs):
    """Execute test function"""
    return {
        "result": f"Test executed with data: {data}",
        "kwargs": kwargs,
        "success": True
    }

def test_function(x, y=10):
    """Simple test function"""
    return x + y
